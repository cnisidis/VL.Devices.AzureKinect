<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
  <metadata>
    <id>VL.Devices.AzureKinect.Femto</id>
    <version>0.0.3-alpha</version>
